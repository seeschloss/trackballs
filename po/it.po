#: src/mmad.cc:106
#, c-format
msgid ""
"Usage: %s [-w, -m] [-e, -l -t <level>] [-r <width>] [-s <sensitivity>]\n"
msgstr ""
"Utilizzo: %s [-w, -m] [-e, -l -t <livello>] [-r <larghezza>] [-s "
"<sensibilità>]\n"

#: src/mmad.cc:107
#, c-format
msgid "   -h  --help            Display this usage information.\n"
msgstr "   -h  --help            Visualizza questa guida.\n"

#: src/mmad.cc:108
#, c-format
msgid "   -e  --edit            Start as level editor.\n"
msgstr "   -e  --edit            Avvia il programma con l'editor di livelli.\n"

#: src/mmad.cc:109
#, c-format
msgid "   -l  --level           Start from level.\n"
msgstr "   -l  --level           Parte dal livello.\n"

#: src/mmad.cc:110
#, c-format
msgid "   -w  --windowed        Run in window (Default is fullscreen)\n"
msgstr ""
"   -w  --windowed        Avvia in una finestra (predefinito tuttoschermo)\n"

#: src/mmad.cc:111
#, c-format
msgid "   -m  --mute            Mute sound.\n"
msgstr "   -m  --mute            Senza audio.\n"

#: src/mmad.cc:112
#, c-format
msgid "   -r  --resolution      Set resolution to 640, 800 or 1024\n"
msgstr "   -r  --resolution      Imposta la risoluzione a 640, 800 o 1024\n"

#: src/mmad.cc:113
#, c-format
msgid "   -s  --sensitivity     Mouse sensitivity, default 1.0\n"
msgstr "   -s  --sensitivity     Sensibilità del mouse, predefinita 1.0\n"

#: src/mmad.cc:114
#, c-format
msgid "   -f  --fps             Displays framerate\n"
msgstr "   -f  --fps             Visualizza i fotogrammi al secondo\n"

#: src/mmad.cc:115
#, c-format
msgid "   -q  --quiet           Do not print anything to stdout\n"
msgstr "   -q  --quiet           Non invia dati a stdout\n"

#: src/mmad.cc:116
#, c-format
msgid "   -v  --version         Prints current version number\n"
msgstr "   -v  --version         Visualizza la versione corrente\n"

#: src/mmad.cc:117
#, c-format
msgid "   -t  --touch           Updates a map to the latest format\n"
msgstr "   -t  --touch           Aggiorna una mappa all'ultimo FileFormat\n"

#: src/mmad.cc:118
#, c-format
msgid "   -y  --low-memory      Attempt to conserve memory usage\n"
msgstr "   -y  --low-memory     Ottimizza l'utilizzo della memoria\n"

#: src/mmad.cc:119
#, c-format
msgid "   -j  --repair-joystick Correct for bad joysticks\n"
msgstr "   -j  --repair-joystick Correzione per joystick difettosi\n"

