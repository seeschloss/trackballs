#: src/mmad.cc:106
#, c-format
msgid ""
"Usage: %s [-w, -m] [-e, -l -t <level>] [-r <width>] [-s <sensitivity>]\n"
msgstr ""
"Utilisation: %s [-w, -m] [-e, -l -t <niveau>] [-r <largeur>] [-s "
"<sensibilite>]\n"

#: src/mmad.cc:107
#, c-format
msgid "   -h  --help            Display this usage information.\n"
msgstr "   -h  --help            Affiche ce message d'aide.\n"

#: src/mmad.cc:108
#, c-format
msgid "   -e  --edit            Start as level editor.\n"
msgstr "   -e  --edit            Lance l'editeur de niveaux.\n"

#: src/mmad.cc:109
#, c-format
msgid "   -l  --level           Start from level.\n"
msgstr "   -l  --level           Commence a ce niveau.\n"

#: src/mmad.cc:110
#, c-format
msgid "   -w  --windowed        Run in window (Default is fullscreen)\n"
msgstr "   -w  --windowed        Mode fenetre (plein ecran par defaut)\n"

#: src/mmad.cc:111
#, c-format
msgid "   -m  --mute            Mute sound.\n"
msgstr "   -m  --mute            Son muet.\n"

#: src/mmad.cc:112
#, c-format
msgid "   -r  --resolution      Set resolution to 640, 800 or 1024\n"
msgstr "   -r  --resolution      Definit la resolution a 640, 800 ou 1024\n"

#: src/mmad.cc:113
#, c-format
msgid "   -s  --sensitivity     Mouse sensitivity, default 1.0\n"
msgstr "   -s  --sensitivity     Sensibilite de la souris, 1.0 par defaut\n"

#: src/mmad.cc:114
#, c-format
msgid "   -f  --fps             Displays framerate\n"
msgstr "   -f  --fps             Affiche le nombre d'images par seconde\n"

#: src/mmad.cc:115
#, c-format
msgid "   -q  --quiet           Do not print anything to stdout\n"
msgstr "   -q  --quiet           N'envoie rien vers la sortie standard\n"

#: src/mmad.cc:116
#, c-format
msgid "   -v  --version         Prints current version number\n"
msgstr "   -v  --version         Affiche la version courante\n"

#: src/mmad.cc:117
#, c-format
msgid "   -t  --touch           Updates a map to the latest format\n"
msgstr "   -t  --touch           Met a jour une carte vers le format actuel\n"

#: src/mmad.cc:118
#, c-format
msgid "   -y  --low-memory      Attempt to conserve memory usage\n"
msgstr "   -y  --low-memory      Essaie de limiter l'utilisation de memoire\n"

#: src/mmad.cc:119
#, c-format
msgid "   -j  --repair-joystick Correct for bad joysticks\n"
msgstr "   -j  --repair-joystick Pour certaines (mauvaises) manettes de jeu\n"

