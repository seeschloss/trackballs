#: src/mmad.cc:106
#, c-format
msgid ""
"Usage: %s [-w, -m] [-e, -l -t <level>] [-r <width>] [-s <sensitivity>]\n"
msgstr ""
"Használat: %s [-w, -m] [-e, -l -t <pálya>] [-r <szélesség>] [-s "
"<érzékenység>]\n"

#: src/mmad.cc:107
#, c-format
msgid "   -h  --help            Display this usage information.\n"
msgstr "   -h  --help            Megjeleníti ezt a használati útmutatót.\n"

#: src/mmad.cc:108
#, c-format
msgid "   -e  --edit            Start as level editor.\n"
msgstr "   -e  --edit            Pályaszerkesztõ indítása.\n"

#: src/mmad.cc:109
#, c-format
msgid "   -l  --level           Start from level.\n"
msgstr "   -l  --level           Indítás pályától.\n"

#: src/mmad.cc:110
#, c-format
msgid "   -w  --windowed        Run in window (Default is fullscreen)\n"
msgstr ""
"   -w  --windowed        Ablakban futtatás (teljes képernyõs az "
"alapértelmezett)\n"

#: src/mmad.cc:111
#, c-format
msgid "   -m  --mute            Mute sound.\n"
msgstr "   -m  --mute            Hang elnémítása.\n"

#: src/mmad.cc:112
#, c-format
msgid "   -r  --resolution      Set resolution to 640, 800 or 1024\n"
msgstr ""
"   -r  --resolution      Felbontás beállítása 640, 800 vagy 1024 értékre.\n"

#: src/mmad.cc:113
#, c-format
msgid "   -s  --sensitivity     Mouse sensitivity, default 1.0\n"
msgstr "   -s  --sensitivity     Egér érzékenység, 1.0 az alapértelmezett.\n"

#: src/mmad.cc:114
#, c-format
msgid "   -f  --fps             Displays framerate\n"
msgstr "   -f  --fps             Képfrissítési gyorsaság megjelenítése.\n"

#: src/mmad.cc:115
#, c-format
msgid "   -q  --quiet           Do not print anything to stdout\n"
msgstr "   -q  --quiet           Ne írjon ki semmit a standard kimenetre.\n"

#: src/mmad.cc:116
#, c-format
msgid "   -v  --version         Prints current version number\n"
msgstr "   -v  --version         Aktuális verziószám kiírása.\n"

#: src/mmad.cc:117
#, c-format
msgid "   -t  --touch           Updates a map to the latest format\n"
msgstr ""
"   -t  --touch           Aktualizálja a pályát a legutóbbi formátumra.\n"

#: src/mmad.cc:118
#, c-format
msgid "   -y  --low-memory      Attempt to conserve memory usage\n"
msgstr "   -y  --low-memory      Kevesebb memóriát használ.\n"

#: src/mmad.cc:119
#, c-format
msgid "   -j  --repair-joystick Correct for bad joysticks\n"
msgstr "   -j  --repair-joystick Hibás botkormány javítása.\n"

#: src/mmad.cc:121
#, c-format
msgid "Important keyboard shortcuts\n"
msgstr "Fontos gyorsbillentyûk\n"

#: src/mmad.cc:122
#, c-format
msgid "   Escape     Soft quit\n"
msgstr "   Escape     Kilépés\n"

#: src/mmad.cc:123
#, c-format
msgid "   CapsLock   Unhide mouse pointer\n"
msgstr "   Caps Lock   Egérmutató megjelenítése\n"

#: src/mmad.cc:124
#, c-format
msgid "   CTRL-q     Quit the game immediatly\n"
msgstr "   CTRL-q     Azonnali kilépés\n"

#: src/mmad.cc:125
#, c-format
msgid "   CTRL-f     Toggle between fullscreen/windowed mode\n"
msgstr "   CTRL-f     Váltás teljes képernyõs/ablakos mód között\n"

#: src/mmad.cc:126
#, c-format
msgid "   k          Kill the ball\n"
msgstr "   k          Játék feladása\n"

#: src/mmad.cc:208
#, c-format
msgid ""
"Commandline switch -e is deprecated, use the editor button the main menu "
"instead\n"
